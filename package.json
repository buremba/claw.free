{
  "name": "claw.free",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "mkdir -p .tmp && process-compose -f process-compose.yaml -U -u .tmp/process-compose.sock up",
    "dev:down": "process-compose -U -u .tmp/process-compose.sock down",
    "dev:status": "process-compose -U -u .tmp/process-compose.sock list",
    "dev:attach": "process-compose -U -u .tmp/process-compose.sock attach",
    "dev:legacy": "concurrently \"vite\" \"NODE_ENV=development tsx watch server/index.ts\"",
    "build": "vite build && tsc -p tsconfig.server.json",
    "start": "NODE_ENV=production node dist-server/index.js",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router": "^1.159.5",
    "@tanstack/router-plugin": "^1.159.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.7.5",
    "lucide-react": "^0.563.0",
    "pg": "^8.13.3",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "shadcn": "^3.8.4",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
